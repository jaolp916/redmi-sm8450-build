    />
    <project 
        name="LineageOS/android_vendor_xiaomi_sm8450-common" 
        path="vendor/xiaomi/sm8450-common" 
        remote="github" 
        revision="lineage-20" 
    />

    <!-- 高通SM8450通用依赖（设备专属，不重复） -->
